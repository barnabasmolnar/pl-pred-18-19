  "private": true,
  "devDependencies": {
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.0",
    "react-scripts": "1.1.4",
    "tailwindcss": "^0.6.5"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "react": "^16.4.1",
    "react-beautiful-dnd": "^8.0.5",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-transition-group": "^2.4.0",
    "redux": "^4.0.0"
  },
  "scripts": {
    "build:css": "postcss src/styles/index.css -o src/index.css",
    "watch:css": "postcss src/styles/index.css -o src/index.css -w",
    "react-dev-server": "react-scripts start",
    "start": "npm-run-all --parallel watch:css react-dev-server",
